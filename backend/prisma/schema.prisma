generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Warehouse {
  A
  B
}

enum Type {
  t20cm
  t40cm
}

model Coil {
  id        Int     @id @default(autoincrement())
  type      Type
  size    Float
  warehouse   Warehouse
  manufactureDate DateTime @default(now())
}