generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum armazem {
  A
  B
}

enum tipo {
  t20cm
  t40cm
}

model Bobina {
  id        Int     @id @default(autoincrement())
  tipo      tipo
  metros    Float
  armazem   armazem
}